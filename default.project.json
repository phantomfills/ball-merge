{
	"name": "ball-merge",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			},
			"assets": {
				"$className": "Folder",
				"balls": {
					"$path": "assets/models/balls"
				}
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/client"
				}
			}
		},
		"Workspace": {
			"$className": "Workspace",
			"$properties": {
				"FilteringEnabled": true
			}, 
			"map": {
				"$path": "assets/models/map.rbxm"
			}
		},
		"HttpService": {
			"$className": "HttpService",
			"$properties": {
				"HttpEnabled": true
			}
		},
		"SoundService": {
			"$className": "SoundService",
			"$properties": {
				"RespectFilteringEnabled": true
			}
		},
		"Lighting": {
			"$className": "Lighting",
			"$properties": {
				"GlobalShadows": true,
				"Ambient": [0.35, 0.35, 0.35],
				"OutdoorAmbient": [0.35, 0.35, 0.35],
				"Brightness": 2,
				"Technology": "ShadowMap",
				"EnvironmentDiffuseScale": 1,
				"EnvironmentSpecularScale": 1,
				"GeographicLatitude": 0,
				"ClockTime": 12
			},
			"$path": "assets/lighting"
		},
		"MaterialService": {
			"$className": "MaterialService",
			"$properties": {
				"Use2022Materials": {
					"Bool": false
				}
			}
		}
	}
}
